<RunSettings>
    <DataCollectionRunSettings>
        <DataCollectors>
            <DataCollector friendlyName="XPlat code coverage">
                <Configuration>
                    <Format>cobertura</Format>
                    <Exclude>[CICDTemplate.Infrastructure]CICDTemplate.Infrastructure.Migrations.*,[CICDTemplate.AppHost]*</Exclude>
                    <Output>./.coverage/coverage.cobertura.xml</Output>
                </Configuration>
            </DataCollector>
        </DataCollectors>
    </DataCollectionRunSettings>
</RunSettings>